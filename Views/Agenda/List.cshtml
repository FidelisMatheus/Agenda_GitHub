@model Agenda_GitHub.Models.ViewModels.AgendaListViewModel

<a asp-action="Create">Adicionar Nova Anotaçao</a>
@foreach (var item in Model.Agendas)
{
    <tr>
        <td>
            <h4>@item.Anotacao </h4>
            <h4>@item.Data </h4>
        </td>


    </tr>
    <div>

        <a asp-action="Edit" asp-route-id="@item.AgendaID">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.AgendaID">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.AgendaID">Delete</a>

    </div>
}
